@model IEnumerable<UImvc.Models.ResultsViewModel>
@{
    ViewData["Title"] = "Admin Index";
}

<h1>Results Administration</h1>

<p>
    <a asp-action="Create">Add Result</a>
</p>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">TestId</th>
            <th scope="col">Name</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Result in Model)
        {
            <tr>
                <td>@Result.Id</td>
                <td>@Result.Name</td>
                <td>
                    <a asp-action="Details" asp-route-id="@Result.Id">Details</a> 
                </td>
            </tr>
        }
    </tbody>
</table>

